<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   
   <HediffDef Name="addedPartBasic" Abstract="True">
      <hediffClass>Hediff_AddedPart</hediffClass>
      <isBad>false</isBad>
      <defaultLabelColor>(148,117,14)</defaultLabelColor>
   </HediffDef>
   
   <!-- Added body parts -->
   
   <HediffDef ParentName="addedPartBasic">
      <defName>IronArm</defName>
      <label>iron arm</label>
	  <labelNoun>an iron arm</labelNoun>
	  <description>An installed iron arm.</description>
      <descriptionHyperlinks><ThingDef>IronArm</ThingDef></descriptionHyperlinks>
      <spawnThingOnRemoved>IronArm</spawnThingOnRemoved>
      <addedPartProps>
         <solid>true</solid>
         <partEfficiency>0.6</partEfficiency>
      </addedPartProps>
      <comps>
         <li Class="HediffCompProperties_VerbGiver">
            <tools>
               <li>
                  <label>iron fist</label>
                  <capacities>
                     <li>Blunt</li>
                  </capacities>
                  <power>10</power>
                  <cooldownTime>1.8</cooldownTime>
               </li>
            </tools>
         </li>
      </comps>
   </HediffDef>
   
