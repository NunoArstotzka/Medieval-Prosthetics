	<RecipeDef Name="SurgeryMedMechanical" ParentName="SurgeryFlesh" Abstract="True">
		<surgerySuccessChanceFactor>1</surgerySuccessChanceFactor>
		<deathOnFailedSurgeryChance>0</deathOnFailedSurgeryChance>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
		<dontShowIfAnyIngredientMissing>true</dontShowIfAnyIngredientMissing>
	</RecipeDef>

	<RecipeDef ParentName="SurgeryMedMechanical">
		<defName>InstallMechanicalMedHand</defName>
		<label>install mechanical hand</label>
		<description>Installs mechanical hand.</description>
		<workerClass>Recipe_InstallArtificialBodyPart</workerClass>
		<jobString>Installing mechanical hand.</jobString>
		<workAmount>350</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>MechanicalMedHand</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>MechanicalMedHand</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Hand</li>
		</appliedOnFixedBodyParts>
		<addsHediff>MechanicalMedHand</addsHediff>
	</RecipeDef>
	
